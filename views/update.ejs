<%- include("partials/header") %> 

    <h1 class="title-update">Update</h1>
    <form action="/update/posts/<%= titlePost %>" method="post">
        <div class="form-group">
            <label for="updateTitle"><%= titlePost %> </label>
        </div>

        <div class="form-group">
            <label for="updatePost">Post</label>
            <textarea name="updatePostValue" class="form-control" id="updatePost" cols="30" rows="10"></textarea>
        </div>
        <button type="submit" class="btn btn-primary">Update</button>
    </form>

<%- include("partials/footer") %>